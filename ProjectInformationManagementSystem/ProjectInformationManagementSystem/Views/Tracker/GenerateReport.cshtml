
@{
    ViewBag.Title = "Report: Genomics Core Laboratory";
}
<h2 style="font-family:'Arial Rounded MT'; font:120;">Generate Report</h2><br />


@using (Ajax.BeginForm("Report", "Tracker", new AjaxOptions { HttpMethod = "POST", InsertionMode = InsertionMode.Replace, UpdateTargetId = "resultArea" } ))
{
    <div class="form-inline row">
        <div class="form-group">
            <label class="control-label" for="startDate">Start Date</label>
            <input type="text" class="form-control datepicker datepicker-buttonpane" id="startDate" placeholder="choose Start Date" name="DateBegin" required="required">
            <i class="fa fa-calendar form-control-feedback"></i>
        </div>

        <div class="form-group">

            <label class="control-label" for="endDate">End Date</label>
            <input type="text" class="form-control datepicker" id="endDate" placeholder="choose Start Date" name="DateEnd" required="required">
            <i class="fa fa-calendar form-control-feedback"></i>
        </div>


    </div>
    <button type="submit" class="btn btn-primary" style ="margin-bottom:10px; margin-top:20px;" >Generate Report</button> 

}


<div id="resultArea">

</div>

